<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head prefix="og: http://ogp.me/ns#">
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">

  <!-- OpenGraph -->
$if(page-title)$
  <meta property="og:title" content="$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$" />
$endif$
  <meta property="og:url" content="$url-prefix$/$filename$"/>
  <meta property="og:type" content="website" />
$if(opengraph-image)$
  <meta property="og:image" content="$url-prefix$/$opengraph-image$" />
$endif$
$if(opengraph-description)$
  <meta property="og:description" content="BunsenLabs Linux APT Repository" />
$endif$

  <!-- Google -->
  <meta name="google" content="notranslate">

$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <link rel="icon" href="img/favicon16.png" sizes="16x16">
  <link rel="icon" href="img/favicon32.png" sizes="32x32">
  <link rel="icon" href="img/favicon48.png" sizes="48x48">
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
$if(dns-prefetch)$
  <link rel="dns-prefetch" href="//www.bunsenlabs.org">
  <link rel="dns-prefetch" href="//forums.bunsenlabs.org">
$endif$
</head>
<body>
<div class="padlock"></div>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$
$for(author)$
<h2 class="author">$author$</h2>
$endfor$
$if(date)$
<h3 class="date">$date$</h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
<script src="js/lock.js"></script>
</body>
</html>
