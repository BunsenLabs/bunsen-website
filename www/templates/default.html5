<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head prefix="og: http://ogp.me/ns#">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(opengraph-description)$
  <meta name="description" content="$opengraph-description$" />
$endif$
  <meta property="og:url" content="$url-prefix$/$filename$"/>
  <meta property="og:type" content="website" />
$if(pagetitle)$
  <meta property="og:title" content="$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$" />
$endif$
$if(opengraph-image)$
  <meta property="og:image" content="$url-prefix$/$opengraph-image$" />
$endif$
$if(opengraph-description)$
  <meta property="og:description" content="$opengraph-description$" />
$endif$
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$for(alternate-css)$
  <link rel="alternate stylesheet" href="$alternate-css$" title="Mercury">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
$if(dns-prefetch)$
  <link rel="dns-prefetch" href="//asia.pkg.bunsenlabs.org">
  <link rel="dns-prefetch" href="//forums.bunsenlabs.org">
  <link rel="dns-prefetch" href="//kelaino.bunsenlabs.org">
  <link rel="dns-prefetch" href="//pkg.bunsenlabs.org">
  <link rel="dns-prefetch" href="//asia.pkg.bunsenlabs.org">
$endif$
  <script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "url": "https://www.bunsenlabs.org",
    "logo":"https://www.bunsenlabs.org/img/blflame512.png"
  }
</script>
</head>
<body>
$for(include-before)$
$include-before$
$endfor$

$if(toc)$
  <div class="toc">
    <div class="content-wrapper">
      $toc$
    </div>
  </div>
$endif$

<main>
  <div class="content-wrapper">
    
    $body$

  </div>
</main>

<footer>
  <div class="footer-sponsor">
        <a href="https://www.netcup.de" title="Sponsored by"><img alt="[NetCup Logo]" src="img/netcup-logo.png"></a>
  </div>
  <div class="content-wrapper">
    $for(include-after)$
    $include-after$
    $endfor$
  </div>
</footer>

</body>
</html>
